{
  "id": "<%= uuid.v4(); %>",
  "name": "Simple Scatter Plot",
  "description": "Example of a scatter plot diagram using D3 framework",
  "version": "<%%= package.version %%>",
  "supportedVersions": "0",
  "moodVersion": "16.0.76",
  "entry": {
    "file": "visualization.js",
    "function": "vis.visualization"
  },
  "supportedBrowsers": "Chrome, Edge, Firefox",
  "inputs": [
    {
      "name": "xAxisMin",
      "displayName": "X Axis Range Minimum",
      "type": "Number",
      "default": 0.0
    },
    {
      "name": "xAxisMax",
      "displayName": "X Axis Range Maximum",
      "type": "Number",
      "default": 10.0
    },
    {
      "name": "yAxisMin",
      "displayName": "Y Axis Range Minimum",
      "type": "Number",
      "default": 0.0
    },
    {
      "name": "yAxisMax",
      "displayName": "Y Axis Range Maximum",
      "type": "Number",
      "default": 10.0
    }
  ],
  "style": {
    "JSON": {
      "margin": {
        "top": 20,
        "bottom": 20,
        "left": 20,
        "right": 20
      },
      "xAxisLabel": "X Axis",
      "yAxisLabel": "Y Axis",
      "spotRadius": 1.5,
      "fillColour": "#69b3a2"
    }
  }
}