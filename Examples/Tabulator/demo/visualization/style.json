{
  "URL": "visualization/style.css",
  "style": {
    "margin": {
      "left": 20,
      "right": 20,
      "top": 20,
      "bottom": 20
    },
    "stylingOptions": {
      "tableSpacing": "medium",
      "initialRow": {
        "enabled": true,
        "title": "ID",
        "frozen": true
      },
      "initialColumnBorders": true,
      "showDetailedTitles": false,
      "theme": "",
      "wrapText": true
    },
    "line": {
      "type": "monotone",
      "strokeColour": "#8884d8"
    },
    "CartesianGrid": {
      "stroke": "#ccc",
      "strokeDasharray": "5 5"
    }
  }
}
