{
  "name": "echarts-tree-chart-visualization",
  "description": "ECharts Radial Tree Visualization",
  "version": "0.0.19",
  "license": "MIT",
  "dependencies": {
    "core-js": "3.19.0",
    "echarts": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "9.0.1",
    "ejs": "3.1.7",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "5.5.0",
    "jest": "^27.3.1",
    "json-loader": "0.5.7",
    "standard": "16.0.4",
    "uuid": "8.3.2",
    "webpack": "^5.54.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "5.8.0",
    "webpack-version-file-plugin": "0.4.0",
    "zip-webpack-plugin": "4.0.1"
  },
  "scripts": {
    "generate-guids": "node generate-guids.js",
    "start": "webpack-dev-server  --config webpack.dev.js --open",
    "build": "npm version patch --no-git-tag-version && webpack --config webpack.prod.js ",
    "rebuild": "webpack --config webpack.prod.js",
    "stats": "webpack --config webpack.prod.js --profile --json > stats.json",
    "check-style": "standard \"src/**/*.js\"",
    "fix-style": "standard --verbose \"src/**/*.js\" --fix",
    "test": "jest",
    "test-init": "jest --init",
    "gen-data": "node gen-data.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "standard": {
    "ignore": [
      "**/__tests__"
    ]
  }
}
